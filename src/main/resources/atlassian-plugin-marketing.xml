<atlassian-plugin-marketing>

	<!-- Names and versions of compatible applications -->
	<compatibility>
		<product name="confluence" min="6.0" max="7.15.0" />
	</compatibility>

	<!-- App logo and banner. The banner is only displayed in the UPM. -->
	<logo image="images/pluginLogo.png" />
	<banner image="images/pluginBanner.png" />

	<!-- Highlight and cropped highlight image assets, in order. -->
	<highlights>
		<highlight image="images/highlight1.png" cropped="images/highlight1_cropped.png" />
		<highlight image="images/highlight2.png" cropped="images/highlight2_cropped.png" />
		<highlight image="images/highlight3.png" cropped="images/highlight3_cropped.png" />
	</highlights>

</atlassian-plugin-marketing>